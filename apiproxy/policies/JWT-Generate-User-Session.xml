<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<GenerateJWT async="false" continueOnError="false" enabled="true" name="JWT-Generate-User-Session">
  <DisplayName>JWT-Generate-User-Session</DisplayName>
  <Type>Signed</Type>
  <Algorithm>HS256</Algorithm>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <SecretKey>
    <Value ref="private.jwt_secret_key" />
    <Id>LOGIN-JWT</Id>
  </SecretKey>
  <ExpiresIn>30m</ExpiresIn>
  <AdditionalClaims>
    <Claim name="idMatch" ref="idMatch" />
    <Claim name="idcdc" ref="idcdc" />
    <Claim name="email" ref="email" />
    <Claim name="primerNombre" ref="primerNombre" />
    <Claim name="segundoNombre" ref="segundoNombre" />
    <Claim name="apellidoPaterno" ref="apellidoPaterno" />
    <Claim name="apellidoMaterno" ref="apellidoMaterno" />
    <Claim name="fechaNacimiento" ref="fechaNacimiento" />
    <Claim name="gratuidad" ref="gratuidad" />
    <Claim name="autorizacionID" ref="autorizacionID" />
    <Claim name="token" ref="token" />
    <Claim name="codigoCupon" ref="codigoCupon" />
    <Claim name="porcentajeDescuento" ref="porcentajeDescuento" />
  </AdditionalClaims>
  <OutputVariable>jwt-variable</OutputVariable>
  <DisplayName>JWT-Generate-HS256</DisplayName>
</GenerateJWT>